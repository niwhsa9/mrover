<launch>
  <arg name="rvizconfig" default="$(find mrover)/config/rviz/navigation.rviz" />
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />

  <!-- <param name="robot_description" command="xacro $(find mrover)/rover_description/rover.xacro" />
  <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" /> -->

  <!-- launch rover core processes -->
  <include file="$(find mrover)/launch/rover_core.launch" />

  <!-- launch the simulator -->
  <include file="$(find mrover)/launch/simulator.launch" />

  <!-- launch auton -->
  <include file="$(find mrover)/launch/auton.launch" />

</launch>